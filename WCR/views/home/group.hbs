                                {{else}}
                                    <td class="column100{{this.className.position4}}">{{this.position4}} ({{this.points.position4}})</td>
                                {{/if}}
                            {{/each}}
                        </tr>

                    {{/each}}

                    <tr class="row200">
                        <td class="column100" colspan="7">Прогноза за световен шампион</td>
                    </tr>

